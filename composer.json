{
    "name": "jesperveldhuizen/findmyiphone",
    "description": "Apple FindMy devices locator",
    "homepage": "https://github.com/jesperveldhuizen/findmyiphone",
    "keywords": ["php", "iphone", "ios", "icloud", "findmyiphone", "findmy"],
    "type": "library",
    "authors": [
        {
            "name": "Jesper Veldhuizen",
            "email": "info@jesperveldhuizen.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "symfony/http-client": "^5.4 | ^6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "2.18.*",
        "phpstan/phpstan": "^1.0",
        "symplify/easy-coding-standard": "9.2.*",
        "symplify/symplify-kernel": "9.2.*"
    },
    "autoload": {
        "psr-4": {
            "Fieldhousen\\FindMy\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
