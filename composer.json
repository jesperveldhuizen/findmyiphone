{
    "name": "jesperveldhuizen/findmyiphone",
    "description": "iPhone locator client",
    "homepage": "https://github.com/jesperveldhuizen/findmyiphone",
    "keywords": ["php", "iphone", "ios", "icloud", "findmyiphone"],
    "type": "library",
    "version": "1.0.1",
    "authors": [
        {
            "name": "Jesper Veldhuizen",
            "email": "info@jesperveldhuizen.com",
            "role": "Development"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "ext-curl": "*",
        "guzzlehttp/guzzle": "^6.0",
        "symfony/http-client": "^6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "2.18.*",
        "phpstan/phpstan": "^1.0",
        "symplify/easy-coding-standard": "9.2.*",
        "symplify/symplify-kernel": "9.2.*"
    },
    "autoload": {
        "psr-4": {
            "FindMyiPhone\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
